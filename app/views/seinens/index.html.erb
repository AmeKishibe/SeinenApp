<p id="notice"><%= notice %></p>

<h1>SeinenJump</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Article</th>
      <th>User name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seinens.each do |seinen| %>
      <tr>
        <td><%= seinen.title %></td>
        <td><%= seinen.article %></td>
        <td><%= seinen.User_name %></td>
        <td><%= link_to 'Show', seinen %></td>
        <td><%= link_to 'Edit', edit_seinen_path(seinen) %></td>
        <td><%= link_to 'Destroy', seinen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seinen', new_seinen_path %>
